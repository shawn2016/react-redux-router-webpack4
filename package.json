{
  "name": "react-router-redux-eslint-parcel",
  "version": "1.0.0",
  "description": "react脚手架,最新技术",
  "main": "index.js",
  "scripts": {
    "start": "parcel src/index-mock.html -p 8888 --open",
    "build": "npm run clear && set NODE_ENV=production && parcel build src/index.html -d build --no-cache --public-url ./",
    "dist": "node server.js",
    "prettier": "prettier --write \"{src,mock}/**/*.{js,css,scss,less}\"",
    "clear": "rimraf ./build",
    "cover": "./node_modules/.bin/istanbul cover _mocha",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | ./node_modules/.bin/coveralls"
  },
  "author": "shawn",
  "license": "ISC",
  "private": true,
  "repository": {
    "type": "git",
    "url": ""
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "dependencies": {
    "antd": "^3.2.3",
    "axios": "^0.18.0",
    "bundle-loader": "^0.5.6",
    "classnames": "^2.2.5",
    "history": "^4.7.2",
    "immutable": "^3.8.2",
    "lodash": "^4.17.5",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-loadable": "^5.3.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "reqwest": "^2.0.5"
  },
  "devDependencies": {
    "autoprefixer": "^8.1.0",
    "babel-eslint": "^8.2.2",
    "babel-plugin-import": "^1.6.5",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "body-parser": "^1.18.2",
    "eslint": "^4.18.2",
    "eslint-plugin-prettier": "^2.6.0",
